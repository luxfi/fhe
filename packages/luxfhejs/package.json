{
  "name": "luxfhejs",
  "version": "0.5.0",
  "description": "LuxFHE JavaScript SDK - Compatibility layer for legacy FHE applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["fhe", "tfhe", "encryption", "blockchain", "lux"],
  "author": "Lux Industries Inc",
  "license": "MIT",
  "dependencies": {
    "ethers": "^6.11.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
